{
  "_version": "1.65.0",
  "sap.app": {
    "id": "scenarios",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:lrop",
      "version": "1.16.1",
      "toolsId": "13e3841b-e241-4b28-a042-3d294f76a473"
    },
    "dataSources": {
      "mainService": {
        "uri": "embedding/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    },
    "crossNavigation": {
      "inbounds": {
        "scenarios-display": {
          "semanticObject": "scenarios",
          "action": "display",
          "title": "RAG(Business Scenarios) ",
          "signature": {
            "parameters": {},
            "additionalParameters": "allowed"
          }
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.135.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.fe.templates": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "scenarios.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      },
      "@i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "uri": "i18n/i18n.properties"
      }
    },
    "resources": {
      "css": []
    },
    "routing": {
      "config": {},
      "routes": [
        {
          "pattern": ":?query:",
          "name": "BusinessScenariosList",
          "target": "BusinessScenariosList"
        },
        {
          "pattern": "BusinessScenarios({key}):?query:",
          "name": "BusinessScenariosObjectPage",
          "target": "BusinessScenariosObjectPage"
        }
      ],
      "targets": {
        "BusinessScenariosList": {
          "type": "Component",
          "id": "BusinessScenariosList",
          "name": "sap.fe.templates.ListReport",
          "options": {
            "settings": {
              "contextPath": "/BusinessScenarios",
              "variantManagement": "Page",
              "navigation": {
                "BusinessScenarios": {
                  "detail": {
                    "route": "BusinessScenariosObjectPage"
                  }
                }
              },
              "controlConfiguration": {
                "@com.sap.vocabularies.UI.v1.LineItem": {
                  "tableSettings": {
                    "type": "ResponsiveTable"
                  }
                }
              },
              "initialLoad": "Enabled"
            }
          }
        },
        "BusinessScenariosObjectPage": {
          "type": "Component",
          "id": "BusinessScenariosObjectPage",
          "name": "sap.fe.templates.ObjectPage",
          "options": {
            "settings": {
              "editableHeaderContent": true,
              "contextPath": "/BusinessScenarios",
              "content": {
                "body": {
                  "sections": {
                    "CDSSection": {
                      "type": "XMLFragment",
                      "template": "scenarios.ext.fragment.viewcategory",
                      "title": "View Category",
                      "position": {
                        "placement": "After",
                        "anchor": "Fields"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "sap.fiori": {
    "registrationIds": [],
    "archeType": "transactional"
  },
  "sap.cloud": {
    "public": true,
    "service": "ai2report.service"
  }
}
