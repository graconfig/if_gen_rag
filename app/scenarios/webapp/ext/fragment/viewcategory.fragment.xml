<core:FragmentDefinition
	xmlns:core="sap.ui.core"
	xmlns="sap.m"
	xmlns:f="sap.ui.layout.form"
>
	<VBox id="_IDGenVBox1">
		<f:Form id="_IDGenForm" editable="true">
			<f:layout>
				<f:ColumnLayout id="_IDGenColumnLayout"
					columnsXL="1"
					columnsL="1"
					labelCellsLarge="12"
				/>
			</f:layout>
			<f:formContainers>
				<f:FormContainer id="_IDGenFormContainer">
					<f:formElements>
						<f:FormElement id="_IDGenFormElement" label="View Category">
							<f:fields>
								<TextArea id="_IDGenTextArea" rows="10"  value="{viewCategory}"
								editable="{= ${ui>/editMode} === 'Editable'}">
                </TextArea>
							</f:fields>
						</f:FormElement>
					</f:formElements>
				</f:FormContainer>
			</f:formContainers>
		</f:Form>
	</VBox>
</core:FragmentDefinition>